#!/usr/bin/env bash

node_modules/.bin/hostile set "`docker-machine ip lacqueristas`" "laurelandwolf.local" &&
node_modules/.bin/hotel start &&
node_modules/.bin/hotel add "http://`docker-machine ip lacqueristas`:3001" --name www.lacqueristas &&
node_modules/.bin/hotel add "http://`docker-machine ip lacqueristas`:3000" --name origin.lacqueristas
